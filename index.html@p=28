<script>
(function() {
    function addStyles (node) {
        var sheet = new CSSStyleSheet();
                        sheet.replaceSync(`
@media (max-width: 768px) {
    [data-testid="uc-default-wall"]{
      height: 67vh;
    }
}
`
    );
        node.shadowRoot.adoptedStyleSheets.push(sheet)
    }


    const el = document.getElementById('usercentrics-root')

    if (el){
        addStyles(el);
        return;
    }

    function callback(mutationsList, observer) {
        for (let mutation of mutationsList) {
            if (mutation.type === 'childList') {
                // Check each added node.
                mutation.addedNodes.forEach(node => {
                    if (node.id === 'usercentrics-root') { // Check if this is the element you're waiting for
                        console.log('Element has appeared in DOM!');
                        // Do something with the element
                        // Optionally, stop observing
                        observer.disconnect();
                        addStyles(node)
                    }
                });
            }
        }
    }

    let observer = new MutationObserver(callback);

    let targetNode = document.documentElement;

    let config = { childList: true, subtree: true };

    observer.observe(targetNode, config);
})()
</script><link rel="icon" href="wp-content/uploads/cropped-w-c-250x250.jpg" sizes="32x32" />
